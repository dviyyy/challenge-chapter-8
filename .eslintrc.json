{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jest/globals" : true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": ["jest"],
    "rules": {
        "class-methods-use-this":"off",
        "no-unused-vars":"off",
        "import/no-dynamic-require":"off",
        "global-require":"off",
        "linebreak-style": 0
    }
}